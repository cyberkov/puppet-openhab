{
  "name": "cyberkov-openhab",
  "version": "0.1.3",
  "author": "Hannes Schaller",
  "summary": "Install and manage openHAB",
  "license": "Apache-2.0",
  "source": "https://github.com/cyberkov/puppet-openhab",
  "project_page": "https://github.com/cyberkov/puppet-openhab",
  "issues_url": "https://github.com/cyberkov/puppet-openhab/issues",
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "7.0",
        "8.0"
      ]
    },
    {
      "operatingsystem": "Raspbian",
      "operatingsystemrelease": [
        "8.0"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04"
      ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 3.2.0 < 5.0.0"
    },
    {
      "name": "puppetlabs-apt",
      "version_requirement": "2.x"
    },
    {
      "name": "puppetlabs-java",
      "version_requirement": "1.x"
    }
  ]
